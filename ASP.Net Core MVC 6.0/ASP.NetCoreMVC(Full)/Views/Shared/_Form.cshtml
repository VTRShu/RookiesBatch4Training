@model ASP.NetCoreMVCEx1.Models.RookieModel
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
           
            <div class="form-group">
                <label asp-for="FirstName" class="control-label"></label>
                <input asp-for="FirstName" class="form-control" />
                <span asp-validation-for="FirstName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="LastName" class="control-label"></label>
                <input asp-for="LastName" class="form-control" />
                <span asp-validation-for="LastName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Gender" class="control-label"></label>
                <div class="custom-control custom-radio">
                    <input type="radio" asp-for="Gender" class="custom-radio" value="Male"/> Male
                </div>
               <div class="custom-control custom-radio">
                <input type="radio" asp-for="Gender" class="custom-radio" value="Female"/> Female
            </div>
            </div>
            <div class="form-group">
                <label asp-for="Email" class="control-label"></label>
                <input asp-for="Email" class="form-control" />
                <span asp-validation-for="Email" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DoB" class="control-label"></label>
                <input asp-for="DoB" asp-format="{0:dd/MM/yyyy}" class="form-control" />
                <span asp-validation-for="DoB"  class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PhoneNumber" class="control-label"></label>
                <input asp-for="PhoneNumber" type="number" class="form-control" />
                <span asp-validation-for="PhoneNumber" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="BirthPlace" class="control-label"></label>
                <input asp-for="BirthPlace" class="form-control" />
                <span asp-validation-for="BirthPlace" class="text-danger"></span>
            </div>
              <div class="form-group">
                <label asp-for="Graduated" class="control-label"></label>
                <div class="custom-control custom-radio">
                    <input type="radio" asp-for="Graduated" class="custom-radio" value=true/> Graduated
                </div>
               <div class="custom-control custom-radio">
                <input type="radio" asp-for="Graduated" class="custom-radio" value=false/> Undergraduate
            </div>
             <div class="form-group">
                <input type="submit" value="Submit" class="btn btn-primary" />
            </div>